<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kgc.dao.UnitMapper">
    <select id="getUnitListByPage" resultType="com.kgc.entity.BaseUnit">
        select *
        from base_unit
        where isDel = 0
    </select>


</mapper>
